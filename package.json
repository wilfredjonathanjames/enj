  },
  "homepage": "https://github.com/wilfredjonathanjames/zli#readme",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "husky": "^9.0.11",
    "np": "^10.0.7",
    "prettier": "^3.3.2",
    "tsx": "^4.16.2",
    "vite-tsconfig-paths": "^4.3.2",
