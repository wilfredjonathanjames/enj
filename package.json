{
  "name": "@wilfredjonathanjames/zli",
  "version": "0.0.1",
  "description": "File Based Router for building CLIs.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilfredjonathanjames/zli.git"
  },
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "prettier": "prettier src/ test/ --write",
    "prettier:check": "prettier src/ test/ --check",
    "prepare": "husky",
    "release": "np"
  },
  "keywords": [
    "cli",
    "file",
    "based",
    "router",
    "typescript",
    "commandline",
    "interface",
    "builder",
    "tool",
    "project",
    "commands"
  ],
  "author": "Wilfred James <hello@wilfredjames.com> (https://github.com/wilfredjonathanjames)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wilfredjonathanjames/zli/issues"
  },
  "homepage": "https://github.com/wilfredjonathanjames/zli#readme",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "husky": "^9.0.11",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "tsx": "^4.16.2",
    "vitest": "^1.6.0"
  },
  "files": [
    "src/",
    "!src/**/*.test.*"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "np": {
    "releaseDraft": false
  },
  "dependencies": {
    "glob": "^11.0.0",
    "zod": "^3.23.8"
  }
